    {
      "cell_type": "code",
      "source": [
        "from typing_extensions import TypedDict"
      ],
      "metadata": {
        "id": "pfGRcY7LrtfP"
      },
      "id": "pfGRcY7LrtfP",
      "execution_count": 227,
