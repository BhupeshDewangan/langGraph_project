          "data": {
            "text/plain": [
              "<langgraph.graph.state.StateGraph at 0x7c455e0ba890>"
            ]
          },
          "metadata": {},
          "execution_count": 220
        }
      ],
      "source": [
        "graph_builder"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 221,
      "id": "e430a904",
      "metadata": {
        "id": "e430a904"
      },
      "outputs": [],
      "source": [
        "def chat_bot(state:State):\n",
        "    return {'messages' : llm.invoke(state['messages'])}"
      ]
